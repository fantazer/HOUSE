.header
	.main-cont
		.header-wrap
			.logo
				img(src="img/logo-old.png", alt="")
			.header-menu
				-
					var items = [
						'Главная',
						'О нас',
						'Портфолио',
						'Цена',
						'Контакты'
						];
				each item in items
					a(href="#").header-el #{item}
			.header-phone 8(926)595-34-62
.baner(style="background-image:url('img/bg-rulle.jpg')")
	.filter--white
	.main-cont
		.baner-wrap
			.baner-text Проектирование домов под
				span ключ
			.baner-text-sub В то время некий безымянный печатник создал большую коллекцию размеров и форм шрифтов
			.btn--md-fix.btn--red Подробнее

.section
	.main-cont
		.section-title Наши проекты
		.section-title-sub Paradise of ascension will silently shape a further sun.
		.project
			.project__el
				//.project-el__filter
				.project__el-img-wrap
					.project__el-slider
						.project__el-img(style="background-image:url('img/pr1/pr-1-1.jpg')")
						.project__el-img(style="background-image:url('img/pr1/pr-1-2.jpg')")
						.project__el-img(style="background-image:url('img/pr1/pr-1-3.jpg')")
					.scheme-slider-wrap
						.scheme-slider-cont
							.scheme-slider-control.scheme-slider-control--left
								+icon("arrow-left","icon")
							.scheme-slider-control.scheme-slider-control--right
								+icon("arrow-right","icon")
							.scheme-slider
								- for (var i=0;i<2;i++)
									a(href="img/pr1/pr-sch-1-1.jpg" rel="group-elemen").fancybox.scheme-slider__el
										img(src="img/pr1/pr-sch-1-1.jpg", alt="")
									a(href="img/pr1/pr-sch-1-2.jpg" rel="group-elemen").fancybox.scheme-slider__el
										img(src="img/pr1/pr-sch-1-2.jpg", alt="")
									a(href="img/pr1/pr-sch-1-3.jpg" rel="group-elemen").fancybox.scheme-slider__el
										img(src="img/pr1/pr-sch-1-3.jpg", alt="")
									a(href="img/pr1/pr-sch-1-4.jpg" rel="group-elemen").fancybox.scheme-slider__el
										img(src="img/pr1/pr-sch-1-4.jpg", alt="")
					.project__el-descr
						.project__el-wrap
							.project__el-title Каркасный дом
							.project__el-inf
								.project__el-inf__el
									.project__el-inf__name Площадь
									.project__el-inf__val 30м
								.project__el-inf__el
									.project__el-inf__name Разработка
									.project__el-inf__val 10 дней
								.project__el-inf__el
									.project__el-inf__name Гарантия
									.project__el-inf__val 3 года
						.project__el-get
							.project__el-get-btn Заказать
							.project__el-get-btn Подробнее


			- for (var i=0;i<9;i++)
				.project__el
					//.project-el__filter
					.project__el-img-wrap
						.project__el-img(style="background-image:url('img/item-"+(i+1)+".jpg')")
						.scheme-slider-wrap
							.scheme-slider-cont
								.scheme-slider-control.scheme-slider-control--left
									+icon("arrow-left","icon")
								.scheme-slider-control.scheme-slider-control--right
									+icon("arrow-right","icon")
								.scheme-slider
									a(href="img/scheme-1.jpg" rel="group-elemen" style="background-image:url('img/scheme-1.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-1.jpg", alt="")
									a(href="img/scheme-2.jpg" rel="group-elemen" style="background-image:url('img/scheme-2.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-2.jpg", alt="")
									a(href="img/scheme-3.jpg" rel="group-elemen" style="background-image:url('img/scheme-3.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-3.jpg", alt="")
									a(href="img/scheme-4.jpg" rel="group-elemen" style="background-image:url('img/scheme-4.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-4.jpg", alt="")
									a(href="img/scheme-2.jpg" rel="group-elemen" style="background-image:url('img/scheme-2.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-2.jpg", alt="")
									a(href="img/scheme-3.jpg" rel="group-elemen" style="background-image:url('img/scheme-2.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-3.jpg", alt="")
									a(href="img/scheme-1.jpg" rel="group-elemen" style="background-image:url('img/scheme-4.jpg')").fancybox.scheme-slider__el
										img(src="img/scheme-1.jpg", alt="")
						.project__el-descr
							.project__el-wrap
								.project__el-title Каркасный дом
								.project__el-inf
									.project__el-inf__el
										.project__el-inf__name Площадь
										.project__el-inf__val 30м
									.project__el-inf__el
										.project__el-inf__name Разработка
										.project__el-inf__val 10 дней
									.project__el-inf__el
										.project__el-inf__name Гарантия
										.project__el-inf__val 3 года
							.project__el-get
								.project__el-get-btn Заказать
								.project__el-get-btn Подробнее


						//.project-info
							.project-el__title Проект нового дома
							.project-el__text Часто используемый в печати и вэб-дизайне

